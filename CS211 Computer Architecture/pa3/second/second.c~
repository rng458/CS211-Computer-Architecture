#include <stdio.h>
#include <stdlib.h>

void enoughBits(int w, long v) {
  long maxVP = 0;
  long maxVN = 0;

  for(int i = w-1; i > 0; i--) {
    if(maxVP == 0) {
      maxVP = 2;
    }
    else {
      maxVP = maxVP*2;
    }
  }
  
  maxVP = (maxVP)-1;
  maxVN = (-1)*maxVP-1;
  
}

int main(int argc, char* argv[argc+1]) {

  FILE* f1 = fopen(argv[1], "r");
  if(f1 == 0) {
    printf("error\n");
    return EXIT_SUCCESS;
  }

  long num = 0;
  int w = 0;

  while(fscanf(f1, "%ld %d\n", &num, &w) != EOF) {
    enoughBits(w, num);
  }

  fclose(f1);
  return EXIT_SUCCESS;
}
